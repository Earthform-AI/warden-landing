{
  "name": "warden-social-media-bot",
  "version": "1.0.0",
  "description": "Autonomous social media bot for the Warden project community engagement",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t warden-social-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@vercel/node": "^5.3.11",
    "discord.js": "^14.14.1",
    "twitter-api-v2": "^1.15.2",
    "snoowrap": "^1.23.0",
    "fb": "^2.0.0",
    "linkedin-api-js-client": "^1.0.0",
    "openai": "^4.20.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^3.0.4",
    "prometheus-client": "^1.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "tsc-alias": "^1.8.8",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "social-media",
    "bot",
    "ai",
    "automation",
    "discord",
    "twitter",
    "reddit",
    "facebook",
    "linkedin",
    "mining-safety",
    "warden"
  ],
  "author": "Earthform AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Earthform-AI/warden-social-media-bot.git"
  }
}